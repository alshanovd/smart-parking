generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model ParkingSpot {
  id         String   @id @default(cuid())
  createdAt  DateTime @default(now())
  latitude   Float
  longitude  Float
  imageUrl   String
  rawText    String?
  parsedData Json?
}
